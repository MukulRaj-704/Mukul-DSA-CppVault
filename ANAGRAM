class Solution {
public:
    bool isAnagram(string s, string t) {
        if(s.size()!=t.size()){
            return false;
        }
        vector<int> hashmap (26,0);
        for(  char c : s){
            hashmap[c-'a']++;
        }
        for( char c : t){
            hashmap[c-'a']--;
            if(hashmap[c-'a']<0){
                return false;
            }
        }
        return true;
    }
};
